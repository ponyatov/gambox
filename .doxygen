PROJECT_NAME           = "gambox"
PROJECT_BRIEF          = "Game development engine /Qt/"
PROJECT_LOGO           = doc/logo.png
HTML_OUTPUT            = docs
OUTPUT_DIRECTORY       =
INPUT                  = README.md doc src inc
INCLUDE_PATH           = inc src
EXCLUDE               += doc/meta
# EXCLUDE               += Q* std*.h
# BUILTIN_STL_SUPPORT    = YES
WARN_IF_UNDOCUMENTED   = NO
RECURSIVE              = YES
USE_MDFILE_AS_MAINPAGE = README.md
GENERATE_LATEX         = NO
FILE_PATTERNS         += *.lex *.yacc
EXTENSION_MAPPING      = lex=C++ yacc=C++ ino=C++
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
LAYOUT_FILE            = doc/DoxygenLayout.xml
